#include "main.h"
/**
 * more_numbers- prints the bumbers form 0 to 14, followed by a new line
 * ten times.
 */
void more_numbers(void)
{
	int i = 1, x = 0, y = 0;

	for (y = 0; y < 10; y++)
	{
		for (i = 1; i <= 10; i++)
		{
			for (x = 0; x <= 4; x++)
			{
				if (i > 0)
					_putchar('0' + i);
				_putchar('0' + x);
			}
		}
		_putchar('\n');
	}
}
